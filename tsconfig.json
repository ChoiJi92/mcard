{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@styles/*": ["src/styles/*"],
      "@shared/*": ["src/components/shared/*"],
      "@remote/*": ["src/remote/*"],
      "@models/*": ["src/models/*"],
      "@constants/*": ["src/constants/index"],
      "@mock/*": ["src/mock/*"],
      "@atoms/*": ["src/atoms/*"],
      "@hooks/*": ["src/hooks/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react"
  },
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ]
}
